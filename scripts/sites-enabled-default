		proxy_pass https://beagleboard.hackster.io;
		proxy_http_version 1.1;
		proxy_set_header Host $host;
		proxy_cache_bypass $http_upgrade;
		#proxy_set_header  X-Real-IP           $proxy_protocol_addr;
