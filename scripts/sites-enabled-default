server {
    if ($host = beagleboard.org) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    error_log syslog:server=unix:/dev/log;
    access_log syslog:server=unix:/dev/log;
	listen 80 default_server;
	listen [::]:80 ipv6only=on;
	server_name beagleboard.org;
        return 302 https://beagleboard.org$request_uri;


}

server {
    error_log syslog:server=unix:/dev/log;
    access_log syslog:server=unix:/dev/log;
	server_name beagleboard.org;
	root /mnt/wordpress;

	location ~ ^/~(.+?)(/.*)?$ {
		alias /home/$1/public_html$2;
		index  index.html index.htm;
		autoindex on;
	}

	# Short links for projects (prefix with '+')
	location ~ ^/\+(.*)$ {
		return 303 https://$server_name/p/p/$1;
	}

	# Short links for blog posts (prefix with '@')
	location ~ ^/\@(.*)$ {
		return 303 https://$server_name/blog/$1;
	}

	location /blog {
		index  index.php;

#		if (!-e $request_filename) {
#			rewrite_log on;
#			#rewrite ^(/[^/]+)(/wp-.*) /blog$2 last;
#			#rewrite ^(/[^/]+)(/.*\.php)(.*) /blog$2$3 last;
#			#rewrite ^/blog/(\d+)/$ /blog/index.php?$1;
#			rewrite /blog/wp-admin$ $scheme://$host$uri/ last;
#			rewrite ^(/[^/]+)?(/wp-.*) $2 last;
#			rewrite ^(/[^/]+)?(/.*\.php) $2 last;
#			rewrite ^/blog/(.*)$ /blog/index.php?$1 last;
#		}

		if (!-e $request_filename) {
			rewrite_log on;
			#rewrite /blog/wp-admin$ /blog/wp-admin/index.php last;
                	rewrite ^(/[^/]+)?(/wp-.*) /blog/$2 last;
                	rewrite ^/blog/(.*)$ /blog/index.php?q=$1 last;
		}

		try_files $uri $uri/ /blog/index.php$is_args$args;

		location ~ \.php$ {
			#try_files $uri =404;
			#fastcgi_split_path_info ^(/blog)(/.*)$;
			#fastcgi_split_path_info ^/blog(.+\.php)\/?(.+)$;
			fastcgi_split_path_info ^(.+\.php)(.+)$;
			fastcgi_pass unix:/var/run/php/php7.4-fpm.sock;
			fastcgi_index index.php;
			include fastcgi_params;
			#fastcgi_param SCRIPT_FILENAME /mnt/wordpress/$fastcgi_script_name;
			fastcgi_param SCRIPT_FILENAME $request_filename;
		}
	}

	location /wp {
		index  index.php;
		rewrite ^/wp/([^/]+)$ /wp/index.php?$1;
		#try_files $uri $uri/ /wp/index.php?$args /wp/index.php?q=$uri&$args;

		location ~ \.php$ {
			try_files $uri =404;
			#fastcgi_split_path_info ^(/wp)(/.*)$;
			#fastcgi_split_path_info ^/wp(.+\.php)\/?(.+)$;
			fastcgi_split_path_info ^(.+\.php)(.+)$;
			fastcgi_pass unix:/var/run/php/php7.4-fpm.sock;
			fastcgi_index index.php;
			include fastcgi_params;
			#fastcgi_param SCRIPT_FILENAME /mnt/wordpress/$fastcgi_script_name;
			fastcgi_param SCRIPT_FILENAME $request_filename;
		}
	}

