                  &lt;source srcset=&quot;/static/beagleV/img/s1-finger.png&quot; type=&quot;image/png&quot;&gt;
                  &lt;img src=&quot;/static/beagleV/img/s1-finger.png&quot; alt=&quot;&quot;&gt;
                  &lt;/picture&gt;
                  &lt;/div&gt;
                  
                  &lt;!-- Truly Open Source --&gt;
