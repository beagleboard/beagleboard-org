                        &lt;div class=&quot;s4-brand-item&quot;&gt;&lt;img src=&quot;/static/beagleV/img/s4-brand-2.png&quot; alt=&quot;&quot;&gt;&lt;/div&gt;
                        &lt;div class=&quot;s4-brand-item&quot;&gt;&lt;img src=&quot;/static/beagleV/img/s4-brand-3.png&quot; alt=&quot;&quot;&gt;&lt;/div&gt;
