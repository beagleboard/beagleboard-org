                    &lt;div id=&quot;s1-icons-trigger&quot;&gt; &lt;/div&gt;
                    &lt;div class=&quot;s1-icon-list-wrap&quot; &gt;
                      &lt;div class=&quot;s1-icon-list-title&quot; id=&quot;s1-icons-title-ani&quot;&gt;&lt;span&gt;Truly Open Source&lt;/span&gt;&lt;/div&gt;
                      &lt;div class=&quot;s1-icon-list&quot; id=&quot;s1-icons-list-ani&quot;&gt;
                        &lt;div class=&quot;s1-icon-item&quot;&gt;
                          &lt;div class=&quot;s1-icon-img&quot; &gt;&lt;img src=&quot;/static/beagleV/img/s1-icon1.png&quot; alt=&quot;&quot;&gt;&lt;/div&gt;
                          &lt;div class=&quot;s1-icon-title&quot;&gt;&lt;span&gt;Open Source Software&lt;/span&gt;&lt;/div&gt;
                        &lt;/div&gt;
                        &lt;div class=&quot;s1-icon-item&quot;&gt;
                          &lt;div class=&quot;s1-icon-img&quot;&gt;&lt;img src=&quot;/static/beagleV/img/s1-icon2.png&quot; alt=&quot;&quot;&gt;&lt;/div&gt;
                          &lt;div class=&quot;s1-icon-title&quot;&gt;&lt;span&gt;Based on RISC-V Open Architecture&lt;/span&gt;&lt;/div&gt;
                        &lt;/div&gt;
                        &lt;div class=&quot;s1-icon-item&quot;&gt;
                          &lt;div class=&quot;s1-icon-img&quot;&gt;&lt;img src=&quot;/static/beagleV/img/s1-icon3.png&quot; alt=&quot;&quot;&gt;&lt;/div&gt;
                          &lt;div class=&quot;s1-icon-title&quot;&gt;&lt;span&gt;Open Hardware Design&lt;/span&gt;&lt;/div&gt;
                        &lt;/div&gt;
                      &lt;/div&gt;
                    &lt;/div&gt;
                    
                  &lt;/div&gt;
                &lt;/div&gt;
              &lt;/div&gt;
            &lt;/section&gt;

            &lt;script&gt;
              var s1Controller = new ScrollMagic.Controller();
        
              //  section 1 
        
                //---&amp;#26631;&amp;#39064;&amp;#21160;&amp;#30011;----
                var tl_s1Title = new TimelineLite();
                tl_s1Title.fromTo(&quot;#s1-title-ani&quot;,.5 ,{scale:0.8},{scale:1});
                tl_s1Title.fromTo(&quot;#s1-intro-ani&quot;,1.0 ,{scale:0.8},{scale:1},&quot;-=0.4&quot;);
                new ScrollMagic.Scene({
                    triggerElement: &quot;#trigger-s1&quot;,
                    reverse:true,
                    duration:40,
                    triggerHook:0,
                    // offset
                  })
                  .setTween(tl_s1Title) 
                      //.addIndicators({&quot;name&quot;:&quot;s1-title-ani&quot;})
                  .addTo(s1Controller);
        
              // ---&amp;#25163;&amp;#25351;&amp;#21160;&amp;#30011;----
                var tl_s1Finger = new TimelineLite();
                tl_s1Finger.fromTo(&quot;#s1-finger-ani&quot;,.8 ,{translateY:80,scale:1.5},{translateY:0,scale:1,ease:Power0.easeIn});   
                
                    new ScrollMagic.Scene({
                        // triggerElement: &quot;#s1-title-ani&quot;,
                        triggerElement: &quot;#trigger-s1&quot;,
                        reverse:true,
                        duration:244,
                        triggerHook:0,
                        // offset
                      })
                      .setTween(tl_s1Finger) 
                      // .addIndicators({&quot;name&quot;:&quot;s1-finger-ani&quot;})
                      .addTo(s1Controller);

                
                //----icon &amp;#21160;&amp;#30011;-----
                var tl_s1Icons = new TimelineLite();
                tl_s1Icons.fromTo(&quot;#s1-icons-title-ani&quot;,4 ,{opacity:0},{opacity:1,ease:Power0.easeInOut});
                tl_s1Icons.fromTo(&quot;#s1-icons-list-ani&quot;,4,{opacity:0},{opacity:1,ease:Power0.easeInOut},&quot;-=2.65&quot;);
                
                    new ScrollMagic.Scene({
                        triggerElement: &quot;#s1-icons-trigger&quot;,
                        reverse:true,
                        duration:parseInt($(&quot;#s1-icons-trigger&quot;).height()),
                        triggerHook:1,
                        // offset
                      })
                      .setTween(tl_s1Icons) 
                      //.addIndicators({&quot;name&quot;:&quot;s1-icons-trigger&quot;})
                      .addTo(s1Controller);  
              
            &lt;/script&gt;

            &lt;!-- s2 --&gt;
