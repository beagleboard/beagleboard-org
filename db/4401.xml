                          &lt;img src=&quot;/static/beagleV/img/s2-board.png&quot; alt=&quot;&quot;&gt;
                          &lt;/picture&gt;
                        &lt;/div&gt;
                        &lt;div class=&quot;s2-circle-tip&quot; id=&quot;s2-tip-ani&quot;&gt;&lt;img src=&quot;/static/beagleV/img/s2-tips.png&quot; alt=&quot;&quot;&gt;&lt;/div&gt;
                      &lt;/div&gt;
                    &lt;/div&gt;
                    &lt;div id=&quot;trigger-s2-circle&quot; &gt;&lt;/div&gt;

                  &lt;/div&gt;

                  
              &lt;/div&gt;
            &lt;/section&gt;

            &lt;script&gt;
            
            /* new fixed */
              var s2Controller = new ScrollMagic.Controller();
              var s2_fixedController = new ScrollMagic.Controller();
              var tl_s2Title = new TimelineLite();
              var tl_s2Circle = new TimelineLite();
              tl_s2Title.fromTo(&quot;#s2-title-ani&quot;,6,{opacity:0,scale:0.7,ease:Power0.easeInOut},{opacity:1,scale:1.2,ease:Power0.easeInOut});
                new ScrollMagic.Scene({
                    triggerElement: &quot;#trigger-s2-title&quot;,
                    reverse:true,
                    duration:$(&quot;#trigger-s2-title&quot;).height(),
                    triggerHook:100,
                  })
                  .setTween(tl_s2Title) 
                  //.addIndicators({&quot;name&quot;:&quot;s2-title-ani&quot;})
                  .addTo(s2Controller);


                  new ScrollMagic.Scene({
                    triggerElement: &quot;#s2-fixed-target&quot;,
                    reverse:true,
                    duration:$(&quot;#s2-fixed-target&quot;).height()*1.4,
                    triggerHook:0
                  })
                  .setPin(&quot;#s2-fixed-target&quot;,{pushFollowers: true})
                  // .addIndicators({&quot;name&quot;:&quot;s2-fixed-ani&quot;,color:&apos;#00b2bf&apos;})
                  .addTo(s2_fixedController);

                  tl_s2Circle.fromTo(&quot;#s2-ic-ani&quot;,2,{scale:3.2,x:-41,y:30},{y:-20});
                  tl_s2Circle.to(&quot;#s2-ic-ani&quot;,11,{x:0,y:0,scale:1},&quot;+=1&quot;);
                  tl_s2Circle.fromTo(&quot;#s2-board-ani&quot;,6,{opacity:0},{opacity:1},&apos;-=10&apos;);
                  tl_s2Circle.fromTo(&quot;#s2-board-ani&quot;,8,{y:60},{y:0},&apos;-=12&apos;);
                  tl_s2Circle.fromTo(&quot;#s2-tip-ani&quot;,3,{opacity:0,ease:Power2.easeIn},{opacity:1,ease:Power2.easeIn},&quot;-=4&quot;);
                  
                  new ScrollMagic.Scene({
                    triggerElement: &quot;#s2-fixed-target&quot;,
                    reverse:true,
                    duration:$(&quot;#s2-fixed-target&quot;).height()*1.4,
                    triggerHook:0.05
                  })
                  .setTween(tl_s2Circle) 
                  // .addIndicators({&quot;name&quot;:&quot;s2-circle-ani&quot;,color:&apos;#00b2bf&apos;})
                  .addTo(s2_fixedController);



